
      <h2>Log in</h2>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div id="wrap">
  <div class="container text-center fondo-login">
    <div class="row">
    <div class="field">
      <%= f.label :Correo %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>
    <div class="field">
      <%= f.label :Contrasena %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>
      <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
      <% end -%>
  
    <div class="actions">
      <%= f.submit "Ingresar" %>
    </div>
  <% end %>
  
  <%= render "customers/shared/links" %>
  </div>
  </div>
</div>